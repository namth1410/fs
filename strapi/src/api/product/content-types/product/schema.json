{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "text",
      "required": true
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "iPhone",
        "iPad",
        "Mac",
        "Watch"
      ],
      "required": true
    },
    "price": {
      "type": "biginteger",
      "required": true
    }
  }
}
